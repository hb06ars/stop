<!--  AUTOR -->
<script>
function modalMensagem(msg){
	document.getElementById("mensagemModal").innerHTML = msg;
	$("#modalMensagem").modal().show();
}
</script>

<div class="modal fade" id="modalMensagem" tabindex="-1" role="dialog" aria-labelledby="TituloModalCentralizado" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content text-center">
      <div class="modal-header">
        <h5 class="modal-title" style="color:dark" id="TituloModalCentralizado">ATENÇÃO</h5>
      </div>
      <div class="modal-body">
        <span style="color:dark" id="mensagemModal"></span>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-danger" data-dismiss="modal">Fechar</button>
      </div>
    </div>
  </div>
</div>
<!--  FINAL AUTOR -->



